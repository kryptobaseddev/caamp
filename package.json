{
  "name": "@cleocode/caamp",
  "version": "0.1.0",
  "description": "Central AI Agent Managed Packages - unified provider registry and package manager for AI coding agents",
  "type": "module",
  "bin": {
    "caamp": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "providers",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "mcp",
    "skills",
    "cli",
    "claude",
    "cursor",
    "windsurf",
    "codex",
    "gemini"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^13.0.0",
    "@clack/prompts": "^0.10.0",
    "picocolors": "^1.1.0",
    "gray-matter": "^4.0.3",
    "simple-git": "^3.27.0",
    "jsonc-parser": "^3.3.1",
    "js-yaml": "^4.1.0",
    "@iarna/toml": "^2.2.5"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "tsup": "^8.3.0",
    "tsx": "^4.19.0",
    "vitest": "^3.0.0",
    "@types/node": "^22.0.0",
    "@types/js-yaml": "^4.0.9"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
